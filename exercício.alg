algoritmo "contageminteligente"

var
   contador: inteiro
inicio
    Escreval(" -----------------------")
    Escreval("  CONTAGEM INTELIGENTE")
    Escreval(" -----------------------")
    contador <-10
    Enquanto(contador >=0)faca
        Escreval(contador ..)
        contador<- contador -2
    FimEnquanto
Fimalgoritmo
