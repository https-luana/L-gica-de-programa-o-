algoritmo "conteAte10"//CONTADOR
var
   contador : Inteiro
inicio
    contador <- 10
    Enquanto (contador <= 100) faca
//ESTRUTURA REPETIÇAO>MENOR OU <MAIOR
        EscrevaL(contador)
        contador <- contador +10//10 EM 10
    FimEnquanto
    Escreval("Terminei de contar")
fimalgoritmo

